{
  "key": "",
  "name": "Starfish",
  "version": "1.0",
  "description": "Create and share course notes!!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "identity",
    "identity.email",
    "https://www.googleapis.com/*",
    "tabs",
    "*://*.appspot.com/*",
    "*://*.cloudshell.dev/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/starfish_32.png"
  },
  "icons": {
    "32": "images/starfish_32.png",
    "512": "images/starfish_512.png"
  },
  "manifest_version": 2,
  "oauth2": {
    "client_id": "506538592562-t2ei82rtds9adtiabnajp983h407kfqp.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/documents"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/ https://kit.fontawesome.com/; object-src 'self'"
}
